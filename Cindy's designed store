SELECT * FROM  CindyStore;
SELECT * FROM CindyStore ORDER BY Product;
SELECT * FROM CindyStore WHERE Quantity_Sold > 100;
SELECT MAX (Quantity_Sold) FROM CindyStore;
SELECT SUM (Quantity_Sold) FROM CindyStore;
SELECT Product, SUM ( Quantity_Sold) FROM CindyStore GROUP BY Product;
SELECT * FROM CindyStore ORDER BY Quantity_Sold;
